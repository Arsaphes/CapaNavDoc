@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/MyLayout.cshtml";
}

@section TitleSection
{
    Accueil
}

@section HeaderSection
{
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
}

@section ContentBody
{
