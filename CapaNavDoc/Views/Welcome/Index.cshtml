        $(document)
            .ready(function() {
                document.getElementById("AdministrationLink").className += "ActiveLink";
            });
    </script>

    @* ReSharper disable Html.PathError *@
    <form id="WelcomeForm" action="/Welcome/InitializeDatabase" method="post">
        <input type="submit" value="Init DB"/>
    </form>
}

